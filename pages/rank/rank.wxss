/* pages/rank/rank.wxss */
.header-bgImage {
  width: 100%;
  height: 450rpx;
  background-color: red;
}

.rank-cell {
  width: 100%;
  height: 150rpx;
  background-color: white;
  border-bottom: 2rpx solid #e5e5e5;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.rank-cell text {
  color: #666;
  font-size: 36rpx;
  margin-left: 32rpx;
}
.rank-cell image {
  width: 15rpx;
  height: 27rpx;
  margin-right: 32rpx;
}