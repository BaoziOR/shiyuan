@import "home-list/home-list-template.wxss";
.more {
  height: 80rpx;
  width: 100%;
  font-size: 24rpx;
  text-align: center;
  line-height: 60rpx;
}

.home-header {
  width: 750rpx;
}

.header-bgImage {
  width: 750rpx;
  height: 120px;
  z-index: -1;
  position: absolute;
}

.topic-week {
  display: flex;
  flex-direction: row;
  margin-top: 30px;
  margin-left: 15px;
}

.topic-week text {
  font-size: 34rpx;
  vertical-align: middle;
}

.topic-week image {
  margin-left: 10px;
  margin-top: 4px;
  width: 68rpx;
  height: 30rpx;
  vertical-align: middle;
}

.focus-topic-view {
  margin-top: 30px;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  position: relative;
}

.focus-topic-view text {
  font-size: 28rpx;
  color: #000;
   width: 286rpx;
 height: 60rpx;
 text-align: center;
 display: block;
}
.focus-topic-view .active-bg{
  position: relative;
  z-index: 9999;
  color:#fff;
}
.focus-topic-view .active-bg{
 background:url(data:image/png;base64,);
 background-size: 100% 100%;
 width: 286rpx;
 height: 60rpx;
}

.topic-kind-view {
  margin-top: 20px;
  background: #f3f3f3;
  height: 60px;
}

.topic-center-view {
  padding-top: 22px;
  margin: 0 100rpx 0 100rpx;
  width: 550rpx;
  height: 25px;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
}

.topic-center-view .kind-view {
  display: flex;
  flex-direction: row;
}

.kind-view.active text{
  color: black;
}

.kind-view text {
  font-size: 15px;
  color: #666;
}

.kind-view .topic-image image{
  width: 15rpx;
  height: 20px;
  margin-left: 15px;
  display: block;
}
.topic-image.hidden {
  display: none;
}

.topic {
  width: 120rpx;
  height: 120rpx;
  border-radius: 100%;
  position: fixed;
  right: 60rpx;
  bottom: 120rpx;
  z-index: 55;
  text-align: center;
  line-height: 120rpx;
   background: rgba(0, 0, 0, 0.5);
  color: #fff;
}
.bi{
  width:69rpx;
  height: 69rpx;
  vertical-align: middle;
}

.topic:active {
  background-color: #ddd;
  color: #000;
}


